{
  "name": "beet-cl",
  "version": "1.1.6",
  "description": "another component library",
  "main": "build/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development",
    "start": "webpack-dev-server --open --mode development",
    "storybook": "start-storybook -p 9001 -c storybook -s ./storybook/public",
    "build-storybook": "build-storybook -c storybook -s ./storybook/public",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "test-bundlesize": "bundlesize",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "react",
    "component",
    "library",
    "beets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/PeterDulworth/beet-cl.git"
  },
  "bugs": {
    "url": "https://github.com/PeterDulworth/beet-cl/issues"
  },
  "homepage": "https://github.com/PeterDulworth/beet-cl#readme",
  "author": "Peter Dulworth",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-backgrounds": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addon-notes": "^5.1.11",
    "@storybook/addon-storysource": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^6.5.13",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.2.0",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.9.0",
    "sass-loader": "^7.2.0",
    "sass-resources-loader": "^2.0.1",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "path": "^0.12.7",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node",
      ".scss"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  }
}
